{
    "version": "0.0.1",
    "name": "Damo App",
    "index": "index.html",
    "main": "./app/app.jsx",
    "output": {
      "path": "build",
      "version": "",
      "app": "app",
      "vendor": "vendor"
    },
    "jsx": true,
    "vendors": [
      "react", 
      "react-dom",
      "react-router",
      "redux",
      "react-redux",
      "react-router-redux",
      "redux-logger",
      "redux-thunk",
      "hoist-non-react-statics",
      "isomorphic-fetch",
      "cuid",
      "recompose",
      "reselect",
      "rxjs",
      "events",
      "seamless-immutable",
      "moment",    
      "redux-promise-middleware",
      "react-redux-loading-bar",

      "damo-core",
      "damo-antd"
    ],
    "provide": {
      "React": "react",
      "ReactDOM": "react-dom"
    },
    "devtool": "",
    "partition": {
      "NODE_ENV": "production"
    },
    "proxy": [{
      "host": "http://api.github.com/",
      "routes": [ "*" ]
    }]
  }